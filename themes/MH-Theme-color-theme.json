			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#72707a"
			}
		},
		{
			"name": "String",
			"scope": [
			  "string",
			  "string.template"
			],
			"settings": {
			  "foreground": "#e5c07b",
			  "fontStyle": "" 
			}
		},
		{
			"name": "Embedded String Begin/End",
			"scope": [
			  "string.embedded.begin",
			  "string.embedded.end",
